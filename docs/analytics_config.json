{
  "analytics": {
    "enable_real_time": true,
    "enable_streaming": true,
    "enable_caching": true,
    "update_interval": 30
  },
  "metrics": {
    "compliance_metrics": true,
    "performance_metrics": true,
    "vendor_metrics": true,
    "system_metrics": true
  },
  "caching": {
    "analytics_cache_ttl": 300,
    "enable_analytics_cache": true,
    "cache_warming": true
  }
}