  "data_collection_settings": {
    "retry_attempts": 3,
    "timeout_seconds": 30,
    "rate_limit_per_minute": 60,
    "batch_size": 100,
    "error_threshold": 0.1,
    "health_check_interval": 300,
    "sync_schedule": {
      "critical_sources": "every_hour",
      "high_priority": "every_4_hours", 
      "medium_priority": "daily",
      "low_priority": "weekly"
    }
  },
  "compliance_mapping": {
    "frameworks": [
      "ISO_27001",
      "SAMA_Framework",
      "NCA_Guidelines", 
      "CITC_Standards",
      "MOH_Standards",
      "CMA_Regulations"
    ],
    "data_classification": {
      "public": "accessible_to_all",
      "internal": "organization_only",
      "confidential": "authorized_personnel",
      "restricted": "specific_clearance_required"
    }
  },
  "alert_configuration": {
    "critical_violations": {
      "notification_channels": ["email", "sms", "slack"],
      "escalation_time": 15,
      "auto_response": true
    },
    "compliance_updates": {
      "notification_channels": ["email", "dashboard"],
      "digest_frequency": "daily"
    },
    "system_health": {
      "notification_channels": ["email", "monitoring"],
      "check_frequency": 300
    }
  }
}
